void main(){
	int a = 5;
	byte c = 4 b;
	bool x = true;
	while( a < 10){		
		print("in first loop\n");
		printi(a);
		print("\n");
		
		while(c < 6 b){
			print("in second loop\n");
			printi(c);
			print("\n");
			if( c == 4 ){
				print("second loop didn't break\n");
				printi(c);
				print("\n");
				c = c + 1 b;				
			}
			else{
				print("break second loop\n");
				printi(c);
				print("\n");
				break;
			}

			print("finished iteration in second loop\n");	
		}
		
		if(not x or a<=7 ){
			print("first loop didn't break\n");
			printi(a);
			print("\n");
		}

		else{
			print("break first loop\n");
			printi(a);
			print("\n");
			break;
		}	
		a = a+1;
		c = c - 1 b;
		print("finished iteration in first loop\n");
	}
	print("bye bye\n");
}