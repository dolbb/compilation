void main(){

	int a = 4;
	byte c = 1 b;

	while( a < 10){		
		
		print("in first loop\n");
		printi(a);
		print("\n");
		
		while(c < 6 b){
			print("in second loop\n");
			printi(c);
			print("\n");
			
			switch(c){
				case 1 b:
				{
					print("inner switch - in case 1\n");
					c= 4 b;
				}break;
				case 4 b:
				{
					print("inner switch - in case 4\n");
					print("break second loop\n");
					break;
				}break;		
			};
		}
			
		print("finish inner switch\n");
		
		switch(a){
			case 1:{
				print("first loop case 1\n");
				print("break first loop\n");
				break;
			}break;
			case 5:
			{
				print("first loop case 5\n");
				a = 0;
			}break;				
		};

		print("finish outer switch\n");
		a = a+1;
	}

	print("bye bye\n");
}