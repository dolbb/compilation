void foo()
{
	print("shouldnt get here\n");
}


void main()
{
	int i = 0;
	int arr[100];
	
	// check arrays initialize to 0
	while (i < 100)
	{
		if (arr[i] != 0)
			foo();
		i = i + 1;
	}
	
	
	// check assignment & reading
	i = 0;
	while (i < 100)
	{
		arr[i] = i;
		i = i + 1;
	}
	
	i = 0;
	while (i < 100)
	{
		if (arr[i] != i)
			foo();
		i = i + 1;
	}
	
	
	// check array to array assignment
	int arr2[100];
	arr2 = arr;
	i = 0;
	while (i < 100)
	{
		if (arr2[i] != arr[i])
			foo();
		i = i + 1;
	}
	
	print("done:)\n");
}