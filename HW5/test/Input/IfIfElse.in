void ifIfElse(bool first, bool second){
	if(first)
		if(second)
			print("Passed first && second :)\n");
		else
			print("Passed first but not second :)\n");			
}

void main(){
	ifIfElse(true, true);
	ifIfElse(true, false);
	ifIfElse(false, true); // Nothing :D
	ifIfElse(false, false); // Nothing :D
}